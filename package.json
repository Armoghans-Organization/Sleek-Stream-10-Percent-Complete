{
  "name": "sleek-stream",
  "version": "1.0.0",
  "description": "Hugo-powered website theme for blogs, portfolios, summery and content manager using TailwindCss.",
  "scripts": {
    "npm-check": "npm-check -u",
    "fullinstall": "npm run preinstall && npm install && npm run postinstall",
    "preinstall": "rimraf assets/vendor",
    "postinstall": "vendor-copy",
    "assets": "rimraf assets/vendor && vendor-copy",
    "start": "hugo server --buildDrafts --buildFuture --buildExpired --gc --ignoreCache --noBuildLock --logLevel info",
    "watch": "hugo server --buildDrafts --buildFuture --buildExpired --gc --ignoreCache --noBuildLock --logLevel info --watch",
    "build": "hugo --gc --verbose --cleanDestinationDir"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.x",
    "@tailwindcss/forms": "^0.x",
    "@tailwindcss/typography": "^0.5.x",
    "autoprefixer": "^10.x",
    "postcss": "^8.x",
    "postcss-cli": "^10.x",
    "tailwindcss": "^3.x"
  },
  "devDependencies": {
    "fuse.js": "^7.0.0",
    "mark.js": "^8.11.1",
    "npm-check": "^6.0.1",
    "rimraf": "^5.0.5",
    "vendor-copy": "^3.0.1"
  },
  "keywords": [
    "Hugo",
    "TailwindCss",
    "Custom",
    "Theme",
    "Custom Theme",
    "Template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Armoghans-Organization/Sleek-Stream.git"
  },
  "author": "Armoghan-ul-Mohmin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Armoghans-Organization/Sleek-Stream/issues"
  },
  "homepage": "https://github.com/Armoghans-Organization/Sleek-Stream#readme",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead",
    "not IE 11",
    "not op_mini all",
    "Firefox ESR",
    "iOS >= 9",
    "Android >= 4.4"
  ],
  "vendorCopy": [],
  "devVendorCopy": [
    {
      "from": "node_modules/fuse.js/dist/fuse.min.js",
      "to": "assets/vendor/fuse.js"
    },
    {
      "from": "node_modules/mark.js/dist/mark.min.js",
      "to": "assets/vendor/mark.js"
    }
  ]
}
